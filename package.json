{
  "name": "typescript-test",
  "description": "test application",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinodseb/typescript-test.git"
  },
  "main": "app/src/index.js",
  "scripts": {
    "copyjson": "copyfiles src/**/*.json app",
    "copyhbs": "copyfiles 'src/**/*.hbs' app",
    "copyhtml": "copyfiles 'src/**/*.html' app",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc && npm run copyjson && npm run copyhbs && npm run copyhtml",
    "prestart": "npm run build",
    "start": "node app/src/index",
    "start-dev": "nodemon .",
    "test": "find src/test -name '*.test.ts' | xargs mocha -r ts-node/register -R spec --exit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.3.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-validator": "^6.6.0",
    "http-status-codes": "^1.4.0",
    "jquery": "^3.5.1",
    "mocha": "^6.2.3",
    "node-cache": "^5.1.2",
    "portfinder": "^1.0.26",
    "sparkpost": "^2.1.4",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.6",
    "typescript-collections": "^1.3.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.7",
    "@types/express-handlebars": "^3.1.0",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/jquery": "^3.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mjml": "^4.0.4",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.7.30",
    "@types/node": "^12.12.48",
    "@types/node-cache": "^4.2.5",
    "@types/sparkpost": "^2.1.4",
    "nodemon": "^2.0.4"
  },
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "bugs": {
    "url": "https://github.com/vinodseb/typescript-test/issues"
  },
  "homepage": "https://github.com/vinodseb/typescript-test#readme"
